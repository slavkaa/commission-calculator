{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "guzzlehttp/guzzle": "^7.8",
    "monolog/monolog": "^3.7",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.11",
    "phpunit/php-code-coverage": "^11.0",
    "phpunit/phpunit": "^11.2"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
  }
}
